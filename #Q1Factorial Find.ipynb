    "def factorial(n):\n",
    "    if n == 0 :\n",
    "        return 1\n",
    "    else :\n",
    "        return n*factorial(n-1)\n",
    "n=int(input(\"Enter a number to compute the Factorial :\"))\n",
    "print(factorial(n))"
